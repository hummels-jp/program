{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug OpenMP Program",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/bin/openmp_example.exe",
        "args": [10],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": true,
        "MIMode": "gdb",
        "miDebuggerPath": "C:\\Program Files\\mingw64\\bin\\gdb.exe",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ]
      }
    ]
  }
  